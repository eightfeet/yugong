{
    "backgroundCommon": {
        "imageUrl": "http: //www.by-health.com/static/index/tvc-jld.png",
        "position": "leftTop",
        "positionX": 0,
        "positionY": 50,
        "backgroundColor": "#fee",
        "repeat": "no-repeat",
        "sizeX": 100,
        "sizeY": 100
    },
    "backgroundGradient": {
        "startColor": "#f00",
        "startTransition": 10,
        "endColor": "#ff0",
        "endTransition": 90,
        "middleColor": "#0f0",
        "middleTransition": 50,
        "gradientDirections": "-45deg"
    },
    "border": {
        "radius": 20,
        "radiusTopLeft": 20,
        "radiusTopRight": 30,
        "radiusBottomLeft": 40,
        "radiusBottomRight": 50,
        "borderColor": "#0f0",
        "borderPosition": "all",
        "borderStyle": "solid",
        "borderWidth": 5
    },
    "boxShadow": {
        "shiftRight": 12,
        "shiftDown": 12,
        "spread": 12,
        "blur": 21,
        "color": "#444",
        "inset": ""
    },
    "textShadow": {
        "shiftRight": 12,
        "shiftDown": 12,
        "blur": 21,
        "color": "#444"
    },
    "font": {
        "fontSize":20,
        "lineHeight":20,
        "color":"#F00",
        "letterSP":20,
        "wordSp":-10,
        "fontWeight": "bold",
        "decoration": "line-through",
        "fontStyle":"italic",
        "align": "center"
    },
    "display": {
        "width": 100,
        "height": 100,
        "zIndex": 100,
        "position": "absolute",
        "left": 10,
        "right": 10,
        "top": 10,
        "bottom": 10
    },
    "transform": {
        "scale": 1.2,
        "rotate": 30,
        "translateX": 20,
        "translateY": 20,
        "skewX": 10,
        "skewY": 10
    }
}



